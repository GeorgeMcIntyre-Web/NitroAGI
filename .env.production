# NitroAGI NEXUS Production Environment Variables for fractalnexusai.space
NITROAGI_ENV=production
NITROAGI_DEBUG=false
NITROAGI_LOG_LEVEL=INFO

# API Configuration
NITROAGI_API_HOST=0.0.0.0
NITROAGI_API_PORT=8000
NITROAGI_DOMAIN=fractalnexusai.space

# 6G Network Optimization
NITROAGI_ENABLE_6G=true
NITROAGI_NETWORK_PROFILE=ultra_low_latency

# Database Configuration (Add your production database URLs)
NITROAGI_REDIS_URL=redis://your-redis-instance:6379
NITROAGI_POSTGRES_URL=postgresql://user:password@your-postgres-host:5432/nitroagi

# AI Model Configuration (Add your API keys)
NITROAGI_OPENAI_API_KEY=your_openai_api_key_here
NITROAGI_ANTHROPIC_API_KEY=your_anthropic_api_key_here
NITROAGI_DEFAULT_LLM_MODEL=gpt-4

# Model Configuration
NITROAGI_TEMPERATURE=0.7
NITROAGI_MAX_TOKENS=1000
NITROAGI_TOP_P=1.0
NITROAGI_FREQUENCY_PENALTY=0.0
NITROAGI_PRESENCE_PENALTY=0.0

# Security
NITROAGI_JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
NITROAGI_JWT_ALGORITHM=HS256
NITROAGI_JWT_EXPIRE_MINUTES=30

# Rate Limiting
NITROAGI_RATE_LIMIT_PER_MINUTE=100
NITROAGI_RATE_LIMIT_BURST=20

# Memory Configuration
NITROAGI_REDIS_POOL_SIZE=20
NITROAGI_CACHE_TTL_SECONDS=3600

# CORS Configuration
NITROAGI_CORS_ORIGINS=["https://fractalnexusai.space", "https://*.fractalnexusai.space"]

# Monitoring
NITROAGI_ENABLE_METRICS=true
NITROAGI_HEALTH_CHECK_INTERVAL=30